#include <iostream>
#include <algorithm>
#include <vector>
#include <list>
#include <deque>
#include <cstring>

#define MAX_N 10
#define MAX_K 6

using namespace std;

int memory[MAX_N][MAX_N][MAX_N][MAX_N][MAX_K];
// 현재 위치 2개 + 깎을 위치 2개 + 깎을 높이

void initMemory(void) {
	memset(memory, -1, sizeof(memory));
}

int Solution(pair<int, int> cur_point, bool already_dig, pair<int,int> dig_point, int dig_height) {
	if (memory[cur_point.first][cur_point.second][dig_point.first][dig_point.second][dig_height] != -1)
	{
		return memory[cur_point.first][cur_point.second][dig_point.first][dig_point.second][dig_height];
	}


}

int main(void) {
	

	return 0;
}
